$ORIGIN .
$TTL 604800	; 1 week
<%= name %>	IN SOA	<%= nameservers[0] %>. <%= zone_contact_dns %>. (
				0          ; serial
				604800     ; refresh (1 week)
				86400      ; retry (1 day)
				2419200    ; expire (4 weeks)
				604800     ; minimum (1 week)
				)
<% nameservers.to_a.each { |ns| %>  IN NS <%= ns %>
<% } %>
